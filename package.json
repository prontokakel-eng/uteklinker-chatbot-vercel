{
  "name": "uteklinker-chatbot",
  "version": "1.0.1",
  "private": true,
  "type": "module",
  "main": "./lib/gate.js",
  "exports": {
    "./lib/*": "./lib/*.js"
  },
  "scripts": {
    "test": "node tests/check-healthz.js && node tests/faq-fuzzy-suite-vercel.js",
    "test:prod": "NODE_ENV=production node tests/faq-fuzzy-suite-vercel.js",
    "pull-env": "node vercel-pull.js",
    "test:faq": "node tests/faq-super-suite.mjs",
    "test:lang": "node tests/detect-lang-fulltest.mjs",
    "version-info": "echo 'ðŸ”¤ ChatBot LangDetect Framework â€” v6.3 stable | v6.4â†’v6.5 roadmap ðŸš€'",
    "test:filters": "node tests/filters-wl-bl.mjs",
    "test:smoke": "npm run -s test:smoke:wlbl && npm run -s test:smoke:formats && npm run -s test:smoke:gate",
    "test:torture:fixtures": "node ./tests/Super-torture-suite.repo.mjs --input ./tests/fixtures --limit 200",
    "lint": "eslint lib scripts --ext .js,.mjs --no-error-on-unmatched-pattern",
    "deadcode": "knip --production --config .knip.json",
    "deps:check": "depcheck --skip-missing=true",
    "graph:cycles": "madge --circular --extensions js,mjs",
    "graph:orphans": "madge --orphans --extensions js,mjs",
    "coverage": "c8 --reporter=text --reporter=lcov npm run test",
    "e2e": "playwright test",
    "audit": "node ./scripts/audit-runner.js",
    "o:scan": "npx knip --production --reporter json > logs/reports/knip-orphans.json",
    "o:grep": "node scripts/grep-dynamic-refs.js",
    "o:classify": "node scripts/classify-orphans.js",
    "o:prepare": "node scripts/move-to-deprecated.js --plan logs/reports/orphans-plan.json",
    "o:revert": "git restore --staged . && git checkout -- .",
    "o:apply": "node scripts/move-to-deprecated.js --plan logs/reports/orphans-plan.json"
  },
  "dependencies": {
    "archiver": "^7.0.1",
    "chalk": "^5.6.2",
    "cheerio": "^1.1.2",
    "csv-parse": "^6.1.0",
    "dotenv": "^17.2.3",
    "fuse.js": "^7.1.0",
    "googleapis": "^126.0.1",
    "node-fetch": "^3.3.2",
    "openai": "^5.23.0",
    "string-similarity": "^4.0.4"
  },
  "devDependencies": {
    "c8": "^10.1.3",
    "depcheck": "^1.4.7",
    "eslint": "^9.38.0",
    "knip": "^5.66.1",
    "madge": "^8.0.0",
    "playwright": "^1.56.1"
  }
}
